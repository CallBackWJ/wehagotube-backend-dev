type User {
  id: ID! @id
  email: String! @unique
  name: String!
  avatar: String
  accessToken: String
  permission: TYPE!
}
enum TYPE{
  ADMIN
  USER
}



type Schedule {
   id: ID! @id
   title:String!
   desc:String!
   status:Status! @default(value: RESERVED)
   startTime:String!
   endTime:String!



}

type Video {
  id: ID! @id
  scheduleId:String! @unique
  youtubeId:String! @unique
  thumbnail:String @default(value: "http://image.zdnet.co.kr/2019/06/25/imc_vkCh0ZOG9SNi6oP8.jpg")
  viewCount:Int! @default(value: 0)
  publishedAt: DateTime!
}
enum Status {
  RESERVED
  READY
  LIVE
  COMPLETED
  PUBLISHED
}

 type TimeLink {
  id: ID! @id
  videoId:String!
  time:Int!
  desc:String!
}